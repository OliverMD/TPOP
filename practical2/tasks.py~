def QuestionOne(a,b,c):
    def max(x,y):
        if x > y:
            return x
        else:
            return y
    return max(max(a,b),c)
def QuestioneOneTest:
    assert QuestionOne(0,1,2) == 2
    assert QuestionOne(2,1,0) == 2
    assert QuestionOne(1,2,0) == 2
    assert QuestionOne(5,5,-7) == 5
